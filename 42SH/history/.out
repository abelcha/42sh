int		add_in_history_file(t_line *line)
{
  int		fd;
  t_history	*tmp;
  t_history	*save;
  int		i;

  i = -1;
  tmp = line->history;
  if ((fd = open("./.42sh_history", (O_RDWR | O_CREAT | O_TRUNC), 0777)) < 0)
    return (FAILURE);
  save = tmp->prev;
  while ((tmp = tmp->next) != line->history)
    {
      if (++i < HISTORY_LIMIT && tmp->data)
	{
	  write(fd, tmp->data, strlen(tmp->data));
	  write(fd, "\n", 1);
	}
      c_free(&(tmp->data));
      x_free(tmp->prev);
    }
  x_free(save);
  x_free(line);
  close(fd);
  return (SUCCESS);
}
